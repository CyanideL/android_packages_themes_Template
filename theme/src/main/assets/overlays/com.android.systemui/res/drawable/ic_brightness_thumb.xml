<?xml version="1.0" encoding="utf-8"?>
<animated-selector android:constantSize="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_pressed="true">
        <bitmap android:gravity="center" android:src="@*android:drawable/scrubber_control_off_mtrl_alpha" android:alpha="?android:disabledAlpha" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:gravity="center" android:src="@*android:drawable/scrubber_control_off_mtrl_alpha" android:alpha="?android:disabledAlpha" />
    </item>
    <item android:state_pressed="true" android:id="@*android:id/pressed">
        <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_005" />
    </item>
    <item android:id="@*android:id/not_pressed">
        <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_000" />
    </item>
    <transition android:toId="@*android:id/pressed" android:fromId="@*android:id/not_pressed">
        <animation-list>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_000" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_001" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_002" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_003" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_004" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_to_pressed_mtrl_005" />
            </item>
        </animation-list>
    </transition>
    <transition android:toId="@*android:id/not_pressed" android:fromId="@*android:id/pressed">
        <animation-list>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_000" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_001" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_002" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_003" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_004" />
            </item>
            <item android:duration="15">
                <bitmap android:gravity="center" android:src="@drawable/scrubber_control_from_pressed_mtrl_005" />
            </item>
        </animation-list>
    </transition>
</animated-selector>